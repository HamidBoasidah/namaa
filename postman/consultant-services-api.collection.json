        {
          "name": "Create Service",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Accept",
                "value": "application/json"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"title\": \"استشارة قانونية\",\n  \"description\": \"استشارة قانونية شاملة في مختلف المجالات\",\n  \"category_id\": 1,\n  \"price\": 150.00,\n  \"duration_minutes\": 60,\n  \"buffer\": 15,\n  \"consultation_method\": \"video\",\n  \"delivery_time\": \"خلال 24 ساعة\",\n  \"auto_accept_requests\": false,\n  \"is_active\": true,\n  \"tags\": [1, 2],\n  \"includes\": [\"تحليل الحالة\", \"تقديم النصيحة\", \"متابعة لمدة أسبوع\"],\n  \"target_audience\": [\"الأفراد\", \"الشركات الصغيرة\"],\n  \"deliverables\": [\"تقرير مكتوب\", \"خطة عمل\"]\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/consultant/services",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "api",
                "consultant",
                "services"
              ]
            },
            "description": "إنشاء خدمة جديدة للمستشار"
          }
        },
